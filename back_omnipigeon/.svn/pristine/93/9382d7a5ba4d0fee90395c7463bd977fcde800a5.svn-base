package com.omni.core.exceptions;

/**
 * This class is {@link OmniException} Omni exception class.
 *
 * @author Omnipigeon
 *
 */
public class OmniException extends Exception {

    public OmniException(String message) {
        super(message);
    }

    public OmniException(String message, Throwable throwable) {
        super(message, throwable);
    }

}
